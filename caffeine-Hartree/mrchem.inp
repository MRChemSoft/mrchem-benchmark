# vim:syntax=sh:

rel_prec = 1.0e-REL_PREC

MPI {
  numerically_exact = false
  share_nuclear_potential = true
  share_coulomb_potential = true
}

MRA {
  min_scale = MIN_SCALE
  max_scale = MAX_SCALE
  corner = [ -1, -1, -1 ]
  boxes = [ 2, 2, 2 ]
}

Derivatives {
  kinetic = ABGV_00
}

Molecule {
  multiplicity = MULTIPLICITY
  angstrom = ANGSTROM
$coords
$end
}

WaveFunction {
  method = Hartree
  restricted = RESTRICTED
}

Properties {
  scf_energy = true
  dipole_moment = true
}

SCF {
  kain = KAIN
  max_iter = MAXIT
  rotation = ROTATION
  canonical = CANONICAL
  orbital_thrs = 1.0e-ORB_THRS
  initial_guess = INITIAL_GUESS
}

